{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-console": 0,
        "indent": ["error", "tab"],
        "no-tabs": 0,
        "import/no-unresolved": 0,
        "no-buffer-constructor": 0
    }
}
